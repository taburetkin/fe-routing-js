{
	"name": "fe-routing-js",
	"version": "0.0.10",
	"description": "javascript front-end routing pack",
	"main": "index.js",
	"module": "index.js",
	"author": "dimtabu <dima.taburetkin@gmail.com>",
	"license": "MIT",
	"scripts": {
		"doc": "jsdoc -t templates/haruki -c jsdoc.config.js index.js Routing.js RoutesManager.js RouteHandler.js RequestContext.js ResponseContext.js config.js utils.js -d console",
		"build": "rollup --config",
		"test": "mocha --require esm --config ./test/.mocharc.raw.json",
		"test-w": "mocha -w --require esm --config ./test/.mocharc.raw.json",
		"test-es5": "mocha --require esm --config ./test/.mocharc.es5.json",
		"test-esm": "mocha --require esm --config ./test/.mocharc.esm.json",
		"coverage": "nyc npm run test",
		"coveralls": "yarn coverage && nyc --reporter=text-lcov npm run test | coveralls"
	},
	"devDependencies": {
		"@babel/core": "^7.6.0",
		"@babel/plugin-transform-runtime": "^7.6.0",
		"@babel/preset-env": "^7.6.0",
		"@babel/register": "^7.6.0",
		"@babel/runtime": "^7.6.0",
		"babel-plugin-external-helpers": "^6.22.0",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"better-docs": "^1.3.3",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"coveralls": "^3.0.6",
		"cross-env": "^5.2.1",
		"eslint": "^6.3.0",
		"eslint-config-prettier": "^6.3.0",
		"eslint-plugin-prettier": "^3.1.1",
		"esm": "^3.2.25",
		"jsdoc": "^3.6.3",
		"jsdoc-baseline": "^0.1.4",
		"mocha": "^6.2.0",
		"module-alias": "^2.2.1",
		"nyc": "^14.1.1",
		"rollup": "^1.23.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-babel-minify": "^9.1.0",
		"sinon": "^7.4.2",
		"sinon-chai": "^3.3.0"
	},
	"dependencies": {
		"core-js": "^3.2.1",
		"regenerator-runtime": "^0.13.3"
	}
